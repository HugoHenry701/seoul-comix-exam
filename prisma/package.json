{
  "name": "migration",
  "version": "1.0.1",
  "description": "package migration",
  "main": "index.js",
  "author": "hugo",
  "license": "MIT",
  "scripts": {
    "migrate": "run-s generate prisma:migrate-dev prisma:migrate",
    "prisma:migrate-dev": "prisma migrate dev --name init",
    "prisma:migrate": "prisma migrate deploy",
    "generate": "prisma generate",
    "prisma:format": "prisma format",
    "prisma:studio": "prisma studio",
    "seed": "prisma db seed",
    "prisma:reset": "prisma db push --force-reset"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./seed.ts"
  },
  "dependencies": {
    "@types/node": "20",
    "npm-run-all": "4.1.5",
    "prisma": "5.16.1",
    "ts-node": "10.9.2",
    "typescript": "5"
  }
}
